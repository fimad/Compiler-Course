let sieve = []#100 in
let i = 0 in
let j = 0 in
for(i:=0; i<100; i:=i+1)
  sieve[i] := i+2
in
for(i:=0; i<100; i:=i+1)
  let n = sieve[i] in
  for(j:=i+n; n != ~1 and j<100; j:=j+n)
    sieve[j] := ~1
  in pass
in
for(i:=0; i<100; i:=i+1)
  if sieve[i] != ~1 then
    print sieve[i]
  else
    pass
in pass
